  "scripts": {
    "build:server": "cd server && npm run build && cd ..",
    "build:client": "webpack --config webpack.config.dev.js",
    "build:client:prod": "webpack --config webpack.config.prod.js",
    "build": "npm run build:client & npm run build:server",
    "build:prod": "npm run build:client:prod & npm run build:server",
    "prebuild:prod": "copy package.json build",
    "publish:prod": "npm run build:prod && cd build && npm pack && npm publish",
    "start": "run-p start:server build:client",
    "start:server": "npm start --prefix ./server",
    "lint": "eslint ./src/** --ext .js,.ts,.tsx --quiet --fix"
  },
